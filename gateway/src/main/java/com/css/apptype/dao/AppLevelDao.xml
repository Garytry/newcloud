<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.css.apptype.dao.AppLevelDao">

	<select id="queryObject" resultType="com.css.apptype.entity.AppLevel">
		select * from BASE_APP_LEVEL
		where APP_LEVEL = #{value}
	</select>

	<select id="queryList" resultType="com.css.apptype.entity.AppLevel">
		select * from BASE_APP_LEVEL
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>

</mapper>